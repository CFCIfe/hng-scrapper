{
  "version": 2,
  "github": {
    "enabled": false
  },
  "builds": [
    {
      "src": "dist/apps/hng-scrapper/main.js",
      "use": "@vercel/node"
    },
    {
      "src": "dist/apps/stage-2/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "dist/apps/hng-scrapper/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "dist/apps/stage-2/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
